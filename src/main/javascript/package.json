{
  "name": "crystalball",
  "version": "0.1.0",
  "private": true,
  "author": "Lawrence Wagerfield <lawrence@wagerfield.com>",
  "engines": {
    "node": "4.3.2",
    "npm": "3.10.7"
  },
  "scripts": {
    "clean": "rm -rf resources/tmp",
    "analyse": "npm run clean && cross-env NODE_ENV=production ./resources/shell_scripts/analyse_frontend.sh",
    "debug": "npm run clean && concurrently --prefix name --names \"app,webpack\" \"nodemon --ext js,jsx,ts,tsx,json,scss --ignore resources/tmp/dist/* --exec npm run buildAndDebugBackend\" \"npm run debugFrontend\"",
    "debugBackend": "cross-env NODE_ENV=development cross-env HTTP_PORT=3000 node ./resources/tmp/dist/server/index.js",
    "debugFrontend": "npm run waitForBackend && npm run startFrontendWithHMR",
    "build": "npm run clean && concurrently --prefix name --names \"backend,frontend\" \"npm run buildBackend\" \"npm run buildFrontend\" && ./resources/shell_scripts/post_build.sh",
    "buildBackend": "cross-env NODE_ENV=development SERVER_TYPE=express ./resources/shell_scripts/build_backend.sh",
    "buildFrontend": "cross-env NODE_ENV=development ./resources/shell_scripts/build_frontend.sh",
    "buildAndDebugBackend": "npm run buildBackend && npm run debugBackend",
    "package": "npm run clean && concurrently --prefix name --names \"backend,frontend\" \"npm run packageBackend\" \"npm run packageFrontend\" && ./resources/shell_scripts/post_build.sh",
    "packageBackend": "cross-env NODE_ENV=production ./resources/shell_scripts/build_backend.sh",
    "packageFrontend": "cross-env NODE_ENV=production ./resources/shell_scripts/build_frontend.sh",
    "start": "concurrently --prefix name --names \"backend,frontend\" \"npm run startBackend\" \"npm run startFrontend\"",
    "startBackend": "cross-env NODE_ENV=production cross-env HTTP_PORT=3000 forever --minUptime 1000 --spinSleepTime 1000 ./resources/tmp/dist/server/index.js",
    "startFrontend": "npm run waitForBackend && http-server  -d false -p 3001 -P http://localhost:3000 ./resources/tmp/dist",
    "startFrontendWithHMR": "cross-env NODE_ENV=development node -r babel-register ./node_modules/.bin/webpack-dev-server --hot --inline --config ./resources/config/webpack.config.browser.dev.js",
    "waitForBackend": "./resources/shell_scripts/wait_for_port.sh 3000",
    "waitForFrontend": "./resources/shell_scripts/wait_for_port.sh 3001"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016",
      "stage-2"
    ],
    "plugins": [
      "ramda"
    ]
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "express": "4.14.0",
    "fuse.js": "3.3.0",
    "isomorphic-fetch": "2.2.1",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.11",
    "moment": "2.22.2",
    "ramda": "0.23.0",
    "react": "15.6.2",
    "react-dom": "15.6.2",
    "react-helmet": "5.0.3",
    "react-input-range": "ljwagerfield/react-input-range-fix",
    "react-redux": "5.0.4",
    "react-router-dom": "4.1.2",
    "react-scroll": "1.5.5",
    "react-transition-group": "2.2.1",
    "react-vega": "4.0.2",
    "react-vega-lite": "2.0.2",
    "redux": "3.6.0",
    "redux-form": "6.7.0",
    "redux-logger": "3.0.1",
    "redux-thunk": "2.2.0",
    "url-parse": "1.4.4",
    "vega-lib": "4.4.0",
    "vega-lite": "3.0.0-rc12"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.3",
    "@types/express": "4.0.35",
    "@types/lodash": "4.14.118",
    "@types/moment": "2.13.0",
    "@types/ramda": "0.0.7",
    "@types/react": "15.6.2",
    "@types/react-dom": "15.5.6",
    "@types/react-helmet": "5.0.2",
    "@types/react-hot-loader": "3.0.5",
    "@types/react-redux": "4.4.40",
    "@types/react-router": "4.0.12",
    "@types/react-router-dom": "4.0.8",
    "@types/react-scroll": "1.5.2",
    "@types/redux-form": "6.6.2",
    "@types/redux-logger": "3.0.0",
    "@types/url-parse": "1.4.1",
    "babel-core": "6.24.1",
    "babel-loader": "6.4.1",
    "babel-plugin-ramda": "1.2.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "bootstrap": "3.3.7",
    "bootstrap-sass": "3.3.7",
    "bootswatch": "thomaspark/bootswatch#v3.3.7",
    "concurrently": "3.4.0",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "forever": "0.15.3",
    "http-server": "0.9.0",
    "jasmine-reporters": "2.2.1",
    "json-loader": "0.5.4",
    "minimist": "1.2.0",
    "node-sass": "4.5.2",
    "nodemon": "1.11.0",
    "normalize.css": "6.0.0",
    "postcss": "5.2.16",
    "postcss-loader": "2.0.5",
    "protractor": "5.1.1",
    "protractor-jasmine2-screenshot-reporter": "0.3.5",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.3",
    "source-map-loader": "0.2.1",
    "source-map-support": "0.4.14",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "typescript": "2.3.2",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-bundle-analyzer": "2.9.0",
    "webpack-dev-server": "2.4.2",
    "webpack-node-externals": "1.5.4"
  }
}
